{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            "args": ["-i",
            "/home/phu/Downloads/demo.mp4",
            "-m_det",
            "/home/phu/Desktop/models/person-detection-retail-0013.xml",
            "-m_reid",
            "/home/phu/Desktop/models/person-reidentification-retail-0288.xml",
            "-out_a",
            "/home/phu/Desktop/test.csv",
            "-location",
            "hawthorn"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    // add the directory where our target was built to the PATHs
                    // it gets resolved by CMake Tools:
                    "name": "PATH",
                    "value": "/build/intel64/Debug/./pedestrain_tracker"
                },
            ],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
